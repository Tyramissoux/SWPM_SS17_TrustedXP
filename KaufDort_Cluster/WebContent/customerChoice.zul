<?page title="Feature und Clusteranzahl" contentType="text/html;charset=UTF-8"?>
<zk>
	<window id="win" title="Clusteranzahl und Featurewahl" border="normal"
		apply="frontend.SelectorVM">
		<vbox hflex="1" vflex="0.2">
			<label value="GewÃ¼nschte Clusteranzahl:" />
			<intbox value="5" id="intBoxCluster"
				constraint="no empty : Feld darf nicht leer sein, no negative: Keine negativen Zahlen, no zero : Bitte einen anderen Wert als 0 waehlen, " />
			<button id="btnGO" label="Start" type="button" />
		</vbox>
		<vbox hflex="1" vflex="0.8">
			<listbox id="listbox" model="${win$composer.featureChoice}"
				checkmark="true" >
				<listhead sizable="true">
					<listheader label="Feature" sort="false" align="left"/>
					<listheader label="Beispiel" sort="false" align="left" />
				</listhead>
				<template name="model">
					<listitem>
						<listcell label="${each.feature}" style="text-align: left"/>
						<listcell label="${each.example}" style="text-align: left"/>
					</listitem>
				</template>
			</listbox>
		</vbox>
	</window>
</zk>